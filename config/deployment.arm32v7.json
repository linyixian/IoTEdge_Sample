            }
          }
        },
        "modules": {
          "detectmask": {
            "version": "1.0",
            "type": "docker",
            "status": "running",
            "restartPolicy": "always",
            "settings": {
              "image": "linyixian.azurecr.io/detectmask:0.0.2-arm32v7",
              "createOptions": "{\"NetworkingConfig\":{\"EndpointsConfig\":{\"host\":{}}},\"HostConfig\":{\"NetworkMode\":\"host\",\"Privileged\":true,\"Binds\":[\"/dev/video0:/dev/video0\"],\"Devices\":[{\"PathOnHost\":\"/dev/video0\",\"PathInContainer\":\"dev/video0\",\"CgroupPermissions\":\"mrw\"}]}}"
            }
          }
        }
      }
    },
    "$edgeHub": {
      "properties.desired": {
        "schemaVersion": "1.0",
        "routes": {
          "detectmaskToIoTHub": "FROM /messages/modules/detectmask/outputs/* INTO $upstream"
